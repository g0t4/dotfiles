## general cd
ealias cdr='cd "$(_repo_root)"' # * favorite

## open
ealias orr='open "$(_repo_root)"' # can't use `or` in fish :)
ealias oh='open .'

####### vscode aliases:
ealias ch='code .' # * favorite
ealias cr='code "$(_repo_root)"' # * favorite

#### zed
ealias zh='zed .'
ealias zr='zed "$(_repo_root)"'

# z
ealias zx='z -x'
